<h1>Log In</h1>
<%= form_tag '/login', method: :post do %>
    
    <div class="form-group">
    <%= label_tag :email %>
    <%= email_field_tag :email, nil%>
    </div>

    <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, nil%>
    </div>
    
    <%= submit_tag "Log In"%>
<% end %>